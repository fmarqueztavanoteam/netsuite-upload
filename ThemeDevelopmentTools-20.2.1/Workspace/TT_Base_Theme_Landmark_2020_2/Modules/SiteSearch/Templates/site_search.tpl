<div class="site-search" data-type="site-search"> 
    <div class="site-search-content">
        <form class="site-search-content-form" method="GET" action="/search" data-action="search">
            <div class="site-search-content-input">
				<div data-view="ItemsSeacher"></div>
				<a class="site-search-input-reset" data-type="search-reset" title="{{translate 'Clear Search'}}" aria-label="{{translate 'Clear Search'}}"><i class="site-search-input-reset-icon" aria-hidden="true"></i></a>
            </div>
            <button class="site-search-button-submit" type="submit" aria-label="{{translate 'Search'}}" title="{{translate 'Search'}}">
                <span>{{translate 'Go'}}</span>
                <i class="site-search-input-icon" aria-hidden="true"></i>
            </button>
        </form>
    </div>
</div>



